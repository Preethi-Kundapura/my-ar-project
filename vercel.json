rm vercel.json  # Remove the existing file

# Recreate vercel.json
echo '{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-eval' 'unsafe-inline' blob: data: *;"
        }
      ]
    }
  ]
}' > vercel.json

